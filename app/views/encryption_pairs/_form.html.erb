<%= simple_form_for(@encryption_pair) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :public_key %>
    <%= f.input :private_key %>
    <%= f.input :test_value %>
    <%= f.input :tested %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
