<h1>Listing shipping_plans</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Shipping base amount</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @shipping_plans.each do |shipping_plan| %>
  <tr>
    <td><%= shipping_plan.name %></td>
    <td><%= shipping_plan.shipping_base_amount %></td>
    <td><%= link_to 'Show', shipping_plan %></td>
    <td><%= link_to 'Edit', edit_shipping_plan_path(shipping_plan) %></td>
    <td><%= link_to 'Destroy', shipping_plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Shipping plan', new_shipping_plan_path %>
